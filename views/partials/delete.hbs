
<h3>Delete</h3>
<form method="POST" action="delete">
    <input type="hidden" name="table" value="{{table}}"></input>
    <label>ID</label> <select name="id">
        <option value=""></option>
        {{#each data}}
        <option value="{{this.[0]}}">{{this.[0]}}</option>
        {{/each}}
    </select>
    <input type="reset">
    <input type="submit">
</form>
