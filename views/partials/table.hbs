<h2>Table</h2>
<table border="1", cellpadding="5">
    <thead>
        <tr>
            {{#each header}}
            <th>
                {{this}}
            </th>
            {{/each}}
        </tr>
    </thead>
    <tbody>
        {{#each data}}
        <tr>
            {{#each this}}
            <td>{{this}}</td>
            {{/each}}
        </tr>
        {{/each}}
    </tbody>
</table>